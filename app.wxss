/**app.wxss**/
page{
  background-color: #ffffff;
}
.light{
  color: #333;
  background-color: #fff;
  width: 100vw;
  height: 100vh;
}

.dark{
  color: #fff;
  background-color: rgba(0, 0, 0, 0.726);
  width: 100vw;
  height: 100vh;
}


.modeFixd{
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  position: fixed;
  top: 80%;
  left: 85%;
}
.modeFixd image{
  width: 100%;
  height: 100%;
}

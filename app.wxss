  url('//at.alicdn.com/t/font_2322877_vw8c8175rzh.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_2322877_vw8c8175rzh.woff') format('woff'),
  url('//at.alicdn.com/t/font_2322877_vw8c8175rzh.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_2322877_vw8c8175rzh.svg#sleeve') format('svg');
